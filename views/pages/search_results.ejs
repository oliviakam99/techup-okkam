  gtag('config', 'G-5N49F1ZJB2');

</script>

</head> 

<body>

    <!-- Navbar which can be reused across other pages -->
    <%- include('../partials/navbar'); %>
  <div class="container">

        <div class="row">
            <% for(let i=0; i<sportsToUser[sportName]?.length; i++) { %>
                <div class="column" style="background-color:#aaa;">
                <div class="search-container">
                    <% let user = sportsToUser[sportName][i]; %> 
                    <a href="profile?name=<%= userProfileData[user].name %>" target="_blank"><h1><%= userProfileData[user].name %></h1></a>
                    <% for(let i=0; i<userProfileData[user].listings.length; i++) { %>
                        <% if (userProfileData[user].listings[i].category == sportName) { %> 
                            <p><%= userProfileData[user].listings[i].name %></p>
                            <img src="<%= userProfileData[user].listings[i].imgUrl %>" width="400">
                            
                           <!---- <a href="hhttps://forms.gle/bmvTy13U52tpffSh7" class="submit-link" target="_blank"></a> -->
                           
                        <% } %>
                    <% } %>
                </div>
                </div>
            <% } %>
            <!-- <div class="column" style="background-color:#aaa;">
                <h2>Column 1</h2>
                <p>Some text..</p>
            </div> -->
        </div>

        
           
    </div>
        
</body>
</html>